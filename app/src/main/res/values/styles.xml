<resources>

    <style name="CommonButton" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:height">60dp</item>
        <item name="android:backgroundTint">@color/color_states_material_button</item>
        <item name="rippleColor">@color/stroke_color</item>
        <item name="android:textColor">@color/white</item>

        <item name="android:fontFamily">@font/inter_medium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.00714286</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="TextMedium_14sp" parent="TextAppearance.Material3.BodyMedium"/>
    <style name="TextMedium_16sp" parent="TextAppearance.Material3.BodyLarge"/>
    <style name="TextDisplayMedium_45sp" parent="TextAppearance.Material3.DisplayMedium"/>
    <style name="TextTitleLarge_22sp" parent="TextAppearance.Material3.TitleLarge"/>
    <style name="TextTitleMedium_16sp" parent="TextAppearance.Material3.TitleMedium"/>
    <style name="TextLabelLarge_14sp" parent="TextAppearance.Material3.LabelLarge"/>
</resources>
